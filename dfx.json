{
  "canisters": {
    "split_dapp": {
      "type": "motoko",
      "main": "icp/src/main.mo",
      "arg": [
        "principal \"2vxsx-fae\"",
        "text \"ckbtc-minter-canister-id\""
      ],
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "split_dapp_test": {
      "main": "icp/test/main_test.mo",
      "type": "motoko",
      "dependencies": [
        "split_dapp"
      ]
    },
    "frontend": {
      "type": "assets",
      "source": [
        "frontend/dist"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:4943",
      "type": "ephemeral"
    },
    "ic": {
      "type": "persistent",
      "providers": [
        "https://icp0.io"
      ]
    }
  },
  "version": 1
}