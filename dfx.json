{
  "canisters": {
    "split_dapp": {
      "type": "motoko",
      "main": "icp/src/main.mo",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "split_dapp_test": {
      "main": "icp/test/main_test.mo",
      "type": "motoko",
      "dependencies": [
        "split_dapp"
      ]
    },

    "frontend": {
      "type": "assets",
      "source": [
        ".next"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:4943",
      "type": "ephemeral"
    },
    "ic": {
      "type": "persistent",
      "providers": [
        "https://icp0.io"
      ]
    },
    "sei_atlantic": {
      "type": "persistent",
      "providers": [
        "https://rpc.atlantic-2.seinetwork.io"
      ]
    },
    "sei_pacific": {
      "type": "persistent",
      "providers": [
        "https://rpc.pacific-1.seinetwork.io"
      ]
    },
    "sei_arctic": {
      "type": "persistent",
      "providers": [
        "https://rpc.arctic-1.seinetwork.io"
      ]
    }
  },
  "version": 1
}